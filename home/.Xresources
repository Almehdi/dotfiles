
Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.autohint: true
Xft.lcdfilter: lcddefault

URxvt*font: \
    xft:PragmataPro:pixelsize=12:antialias=true,xft:Inconsolata for Powerline:pixelsize=11:antialias=true,\
    xft:SauceCodePro NF:pixelsize=9, \
    xft:Sauce Code Powerline:pixelsize=10:antialias=true,\
    xft:FontAwesome:size=8, \
    xft:SourceCodePro:size=9,\
    xft:DejaVuSansMono:size=11,\
    xft:DroidSansMono:size=11,\
    xft:Inconsolata:size=12,\
    xft:Symbola:size=9,\
URxvt*boldFont: xft:Sauce Code Powerline:size=9:antialias=true

URxvt*letterSpace: 0

URxvt*saveLines: 10000
!! #URxvt*transparent: true
URxvt*depth: 32
URxvt*shading: 26

!! scrollbar style - rxvt (default), plain (most compact), next, or xterm
URxvt*scrollBar: false
URxvt*scrollstyle: plain
URxvt*scrollWithBuffer: true
URxvt*scrollBar_right: false
URxvt*urgentOnBell: True

!! URxvt*perl-ext: default,url-select,keyboard-select,clipboard,vtwheel
URxvt*perl-ext: default,matcher,keyboard-select,vtwheel
!!!!URxvt*perl-ext: default,keyboard-select,matcher,clipboard,vtwheel
!! URxvt*url-select.launcher: /usr/bin/firefox -new-tab
URxvt*url-launcher: /usr/bin/firefox -new-tab
URxvt*url-select.underline: true
URxvt*matcher.button:   1
!! URxvt*clipboard.autocopy: true
URxvt*selection-to-clipboard: true
URxvt*selection.pattern-0: ([[:word:]]+)

!! URxvt*keysym.M-c: perl:clipboard:copy
!! URxvt*keysym.M-v: perl:clipboard:paste
!! URxvt*keysym.M-C-v: perl:clipboard:paste_escaped
URxvt*keysym.M-a: perl:keyboard-select:activate
URxvt*keysym.M-s: perl:keyboard-select:search
!! URxvt*keysym.M-u: perl:url-select:select_next
URxvt*keysym.M-u: matcher:select

xterm*termName: xterm-256color
xterm*saveLines: 4096
xterm*utf8: true
Xterm*alternateScroll: true

!! Rofi Colors bg,fg,albg,hlbg,hlfg (rofi -dump-Xresources)
rofi*color-enabled: true
rofi*color-window: #2a2a29, #ac7311, #ac7311
rofi*color-normal: #2a2a29, #cccccc, #353535, #ac7311, #2a2a29
rofi*color-active: #2a2a29, #268bd2, #353535, #268bd2, #ffffff
rofi*color-urgent: #2a2a29, #dc322f, #353535, #dc322f, #ffffff
rofi*font: PragmataPro NF 9
rofi*bw: 1
rofi*opacity: 90
rofi*width: 50

xterm*foreground: rgb:b2/b2/b2
xterm*background: rgb:08/08/08

URxvt*background: rgba:0000/0000/0200/B700
URxvt*foreground: [100]#ffffff

!black
*color0:	#191919
*color8:	#484848
!red
